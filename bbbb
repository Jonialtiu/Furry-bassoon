#include <iostream>
#include <cmath>
using namespace std;
 
int main()
{
	int n;
	cin >> n;
	int* arr = new int[n];
	int* arr1 = new int[n];
	for (size_t i = 0; i < n; i++)
	{
		cin >> arr[i];
	}
	for (size_t i = 0; i < n; i++)
	{
		cin >> arr1[i];
	}
	int a1 = 0;
	int a2 = 0;
	for (size_t i = 0; i < n; i++)
	{
		a1 += (arr[i] == 1 && arr1[i] == 0) ? 1 : 0;
	}
	for (size_t i = 0; i < n; i++)
	{
		a2 += (arr1[i] == 1 && arr[i] == 0) ? 1 : 0;
	}
	if (a1 == 0)
	{
		cout << -1;
		return 0;
	}
	else
	{
		cout << ((a2%a1)==0 ? ceil(a2 * 1.0 / a1)+1 : ceil(a2 * 1.0 / a1));
	}
}
