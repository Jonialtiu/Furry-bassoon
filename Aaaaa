#include <iostream>
using namespace std;
typedef long long bon;
 
bon x, y, c;
 
void solve() {
	cin >> x >> y >> c;
	if(x >= c) 
		cout << "-1 " << y << endl;
	else if(c / y >= x) 
		cout << "1 -1" << endl;
	else 
		cout << "1 " << y << endl;
}
int main() {
	int t;
	cin >> t;
	while(t--) {
		solve();
	}
	
	return 0;
}
